String multiplication(int multiple) {
  String result;
  result = String(multiple) + "\t*\t1\t=\t" + String(multiple * 1) + "\n" +
           String(multiple) + "\t*\t2\t=\t" + String(multiple * 2) + "\n" +
           String(multiple) + "\t*\t3\t=\t" + String(multiple * 3) + "\n" +
           String(multiple) + "\t*\t4\t=\t" + String(multiple * 4) + "\n" +
           String(multiple) + "\t*\t5\t=\t" + String(multiple * 5) + "\n" +
           String(multiple) + "\t*\t6\t=\t" + String(multiple * 6) + "\n" +
           String(multiple) + "\t*\t7\t=\t" + String(multiple * 7) + "\n" +
           String(multiple) + "\t*\t8\t=\t" + String(multiple * 8) + "\n" +
           String(multiple) + "\t*\t9\t=\t" + String(multiple * 9) + "\n" +
           String(multiple) + "\t*\t10\t=\t" + String(multiple * 10) + "\n" +
           String(multiple) + "\t*\t11\t=\t" + String(multiple * 11) + "\n" +
           String(multiple) + "\t*\t12\t=\t" + String(multiple * 12);

  return result;
}
